{
    "name": "kickstart-fastify",
    "version": "1.0.4",
    "description": "",
    "main": "index.js",
    "_moduleAliases": {
        "@": "dist/src"
    },
    "scripts": {
        "dev": "npm run env.dev && tsc-watch --noClear -p ./tsconfig.json --onSuccess \"node ./dist/src/index.js\"",
        "test": "npm run env.test && npm run prisma.reset && tsc && tap",
        "start": "tsc && node ./dist/src/index.js",
        "lint": "eslint --fix ./",
        "prisma.migrate": "prisma migrate dev",
        "prisma.seed": "prisma db seed",
        "prisma.reset": "prisma migrate reset --force --skip-seed && prisma db push && prisma db seed",
        "script.keygen": "ts-node ./scripts/keygen.ts",
        "script.sdkgen": "ts-node ./scripts/sdkgen.ts",
        "env.dev": "node -e \"fs.copyFileSync('.env.dev', '.env')\"",
        "env.test": "node -e \"fs.copyFileSync('.env.test', '.env')\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/KaungWai/kickstart-fastify.git"
    },
    "author": "kaungwai",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/KaungWai/kickstart-fastify/issues"
    },
    "homepage": "https://github.com/KaungWai/kickstart-fastify#readme",
    "dependencies": {
        "@fastify/auth": "^4.2.0",
        "@fastify/autoload": "^5.6.0",
        "@fastify/cookie": "^8.3.0",
        "@fastify/cors": "^8.2.0",
        "@fastify/helmet": "^10.1.0",
        "@fastify/jwt": "^8.0.0",
        "@fastify/sensible": "^5.2.0",
        "@fastify/swagger": "^8.2.1",
        "@fastify/swagger-ui": "^1.3.0",
        "@prisma/client": "^4.7.1",
        "@sinclair/typebox": "^0.24.47",
        "@types/bcrypt": "^5.0.0",
        "bcrypt": "^5.1.0",
        "crypto-js": "^4.1.1",
        "dotenv": "^16.0.3",
        "dotenv-expand": "^9.0.0",
        "env-schema": "^5.1.0",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "fastify": "^4.10.2",
        "fastify-plugin": "^4.4.0",
        "http-status-codes": "^2.2.0",
        "module-alias": "^2.2.2",
        "nanoid": "^3.3.4"
    },
    "devDependencies": {
        "@faker-js/faker": "^7.6.0",
        "@types/crypto-js": "^4.1.1",
        "@types/module-alias": "^2.0.1",
        "@types/node": "^18.11.0",
        "@typescript-eslint/eslint-plugin": "^5.46.1",
        "@typescript-eslint/parser": "^5.46.1",
        "eslint": "^8.30.0",
        "pino-pretty": "^9.1.1",
        "prettier": "2.8.1",
        "prisma": "^4.7.1",
        "swagger-typescript-api": "^12.0.4",
        "tap": "^18.7.0",
        "ts-node": "^10.9.1",
        "tsc-watch": "^6.0.0",
        "typescript": "^4.8.4"
    },
    "prisma": {
        "seed": "ts-node ./prisma/seed.ts"
    }
}
